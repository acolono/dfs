@{ Layout = null;}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title></title>
</head>
<body>
score: <span id="score"></span>
<script src="https://unpkg.com/mqtt@2.18.2/dist/mqtt.min.js"></script>
<script>
    var client = mqtt.connect("wss://iot.eclipse.org:443/ws");
    client.on("connect", function() {
        client.subscribe('aes-2018/digital-fail-state/score');
    });
    client.on('message', function(topic, message, pkg) {
        var score = parseInt(message.toString());
        document.getElementById('score').innerHTML = score;
    });
</script>
</body>
</html>